{
  "name": "problemify",
  "version": "0.5.1",
  "description": "Create a problem and a solution version of a repository.",
  "engines": {
    "node": ">=4.2.4"
  },
  "scripts": {
    "build": "babel src/ --out-dir dist/",
    "clean": "rm -rf dist/ test/fixtures/mockUtils-{problem,solution}",
    "lint": "eslint --fix spec/ src/",
    "prepublish": "npm run test",
    "test": "npm run build && jasmine && npm run lint",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "problemify": "./dist/bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bcmarinacci/problemify.git"
  },
  "keywords": [
    "hack reactor",
    "makersquare",
    "problem",
    "problemify",
    "reactor core",
    "solution",
    "sprint",
    "strip comments"
  ],
  "author": "Benjamin Marinacci",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bcmarinacci/problemify/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "jasmine": "^2.4.1"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "chalk": "^1.1.3",
    "meow": "^3.7.0",
    "ncp": "^2.0.0",
    "pify": "^2.3.0"
  }
}
