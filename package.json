{
  "name": "problemify",
  "version": "0.5.2",
  "description": "Create a problem and a solution version of a repository.",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.6"
  },
  "scripts": {
    "clean": "rm -rf test/fixtures/mockUtils-{problem,solution}",
    "lint": "eslint --fix spec/ src/",
    "prepublish": "npm run test",
    "test": "jasmine && npm run lint",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "problemify": "./bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bcmarinacci/problemify.git"
  },
  "keywords": [
    "assignment",
    "cli",
    "delete",
    "hack reactor",
    "makersquare",
    "multiline",
    "parse",
    "problem",
    "reactor core",
    "remove",
    "solution",
    "sprint",
    "strip",
    "trim"
  ],
  "author": "Benjamin Marinacci",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bcmarinacci/problemify/issues"
  },
  "devDependencies": {
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "jasmine": "^2.4.1"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "meow": "^3.7.0",
    "ncp": "^2.0.0",
    "pify": "^2.3.0"
  }
}
