{
  "name": "problemify",
  "version": "0.5.8",
  "description": "Create a problem and a solution version of a directory",
  "engines": {
    "node": "^6.1.0",
    "npm": "^3.8.6"
  },
  "bin": {
    "problemify": "./bin/cli.js"
  },
  "scripts": {
    "clean": "rm -rf test/fixtures/utils-{problem,solution}",
    "lint": "eslint --fix bin/ lib/ test/",
    "prepublish": "npm run test",
    "test": "istanbul cover tape -- test/**/*.test.js && npm run lint",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "assignment",
    "cli",
    "delete",
    "hack reactor",
    "makersquare",
    "multiline",
    "parse",
    "problem",
    "reactor core",
    "remove",
    "solution",
    "sprint",
    "strip",
    "trim"
  ],
  "author": "Benjamin Marinacci",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcmarinacci/problemify.git"
  },
  "bugs": {
    "url": "https://github.com/bcmarinacci/problemify/issues"
  },
  "files": [
    "bin",
    "lib"
  ],
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "istanbul": "^0.4.3",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "args": "^1.0.2",
    "chalk": "^1.1.3",
    "lodash.flattendeep": "^4.2.0",
    "meow": "^3.7.0",
    "ncp": "^2.0.0",
    "pify": "^2.3.0",
    "trim-whitespace": "^1.1.6"
  }
}
