{
  "name": "problemify",
  "version": "0.5.5",
  "description": "Create a problem and a solution version of a repository.",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.6"
  },
  "bin": {
    "problemify": "./bin/cli.js"
  },
  "scripts": {
    "clean": "rm -rf test/fixtures/mockUtils-{problem,solution}",
    "lint": "eslint --fix bin/ lib/ test/",
    "prepublish": "npm run test",
    "test": "tape test/**/*.test.js | faucet && npm run lint",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "author": "Benjamin Marinacci",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcmarinacci/problemify.git"
  },
  "bugs": {
    "url": "https://github.com/bcmarinacci/problemify/issues"
  },
  "keywords": [
    "assignment",
    "cli",
    "delete",
    "hack reactor",
    "makersquare",
    "multiline",
    "parse",
    "problem",
    "reactor core",
    "remove",
    "solution",
    "sprint",
    "strip",
    "trim"
  ],
  "files": [
    "bin",
    "lib"
  ],
  "devDependencies": {
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "faucet": "0.0.1",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "meow": "^3.7.0",
    "ncp": "^2.0.0",
    "pify": "^2.3.0"
  }
}
