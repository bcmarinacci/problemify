{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "es6": true,
    "jasmine": true,
    "node": true
  },
  "extends": "airbnb",
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "comma-dangle": "off",
    "func-names": "off",
    "max-len": "off",
    "no-console": "off"
  }
}
